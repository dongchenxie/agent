# Master server URL
MASTER_URL=http://localhost:3000

# Shared secret for agent registration (must match master's AGENT_SECRET)
AGENT_SECRET=change-me-in-production

# Unique nickname for this agent
AGENT_NICKNAME=agent-local-01

# Polling and sending configuration (optional, can be configured from master UI)
# POLL_INTERVAL=60000  # How often to poll for tasks (ms), default: 60000 (1 minute)
# SEND_INTERVAL=2000   # Delay between sending emails (ms), default: 2000 (2 seconds)
# BATCH_SIZE=10        # Number of emails to fetch per poll, default: 10
